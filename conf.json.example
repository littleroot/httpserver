{
	"domains": ["littleroot.org", "foo.com", "sub.foo.com"],
	"proxy": {
		"littleroot.org": "http://localhost:8000",
		"foo.com": "http://localhost:9000",
		"sub.foo.com": "https://external.example.com/foo/sub"
	},
	"certs": {
		"auto": false,
		"certFile": "path/to/cert.pem",
		"keyFile": "path/to/key.pem"
	},
	"wellKnown": "path/to/well-known/"
}

